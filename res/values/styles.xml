<?xml version="1.0" encoding="utf-8"?>
<resources>
<!--by boba 18.08.2020
style -->
    <style name="iconText">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/text_gray</item>
    </style>
    <style name="iconTextWhite">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="LauncherTheme" parent="@android:style/Theme.Light.NoTitleBar.Fullscreen">
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
    <style name="Theme" parent="@android:style/Theme.Holo.Wallpaper.NoTitleBar">
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="DialogTheme" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
    </style>
    <style name="compassFrame">
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="compactCompass">
        <item name="android:layout_width">fill_parent</item>
    </style>
    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />
    <style name="AppTheme" parent="@style/AppBaseTheme" />
</resources>
